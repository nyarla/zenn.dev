{
  "name": "zenn.dev",
  "type": "module",
  "private": true,
  "scripts": {
    "up": "tmux new-window && tmux send-keys '(sleep 1 && xdg-open http://nixos:1313/) & ; bun run serve' C-m && tmux select-window -t 1 && nvim .",
    "serve": "zenn preview --port 1313",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyarla/zenn.dev.git"
  },
  "bugs": {
    "url": "https://github.com/nyarla/zenn.dev/issues"
  },
  "homepage": "https://github.com/nyarla/zenn.dev#readme",
  "dependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^19.8.1",
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "^2.0.1",
    "@textlint-ja/textlint-rule-no-insert-re": "^1.0.2",
    "@textlint-ja/textlint-rule-no-synonyms": "^1.3.0",
    "@textlint-rule/textlint-rule-no-unmatched-pair": "^2.0.4",
    "husky": "^9.1.7",
    "sudachi-synonyms-dictionary": "^15.0.0",
    "textlint": "^15.2.2",
    "textlint-rule-date-weekday-mismatch": "^1.1.1",
